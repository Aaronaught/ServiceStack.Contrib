    <iconUrl>http://www.servicestack.net/logo-100x100.png</iconUrl>
    <tags>ServiceStack Authentication Authorization NHibernate SQLServer Sqlite Oracle MySql PostgreSQL POCO Code-First</tags>
    <language>en-US</language> 
    <copyright>ServiceStack 2012 and contributors</copyright>
    <dependencies>
        <dependency id="ServiceStack" />
        <dependency id="Iesi.Collections" />
        <dependency id="NHibernate" />
        <dependency id="FluentNHibernate" />
    </dependencies>
</metadata>
</package>
